<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Force Format With Mixing/Qualification/Final" preserve-order="true" parallel="tests" thread-count="5">
    <test name="Mixing/Qualification/Final" group-by-instances="true">
        <classes>
            <class name="com.qc.competition.service.CompetitionGeneratorConfiguration">
                <methods>
                    <include name="testComputationRankingParticipantMixingQualificationFinal"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Qualification/Final" group-by-instances="true">
        <classes>
            <class name="com.qc.competition.service.CompetitionGeneratorConfiguration">
                <methods>
                    <include name="testComputationRankingParticipantQualificationFinal"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Mixing/Final" group-by-instances="true">
        <classes>
            <class name="com.qc.competition.service.CompetitionGeneratorConfiguration">
                <methods>
                    <include name="testComputationRankingParticipantMixingFinal"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Mixing/Qualification" group-by-instances="true">
        <classes>
            <class name="com.qc.competition.service.CompetitionGeneratorConfiguration">
                <methods>
                    <include name="testComputationRankingParticipantMixingQualification"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Final" group-by-instances="true">
        <classes>
            <class name="com.qc.competition.service.CompetitionGeneratorConfiguration">
                <methods>
                    <include name="testComputationRankingParticipantFinal"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Qualification" group-by-instances="true">
        <classes>
            <class name="com.qc.competition.service.CompetitionGeneratorConfiguration">
                <methods>
                    <include name="testComputationRankingParticipantQualification"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Mixing" group-by-instances="true">
        <classes>
            <class name="com.qc.competition.service.CompetitionGeneratorConfiguration">
                <methods>
                    <include name="testComputationRankingParticipantMixing"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>